{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    },"api/parse/live/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "/",  "dest": "api/index.php" },
    { "src": "/live/(.*).php",  "dest": "api/parse/live/$1.php" },
    { "src": "/PLTV/(.*).php",  "dest": "api/parse/live/$1.php" },
    { "src": "/TVOD/8888/(.*)/(.*).m3u8?playseek=(.*)",  "dest": "api/parse/live/$1.php?id=$2&playseek=$3" },
  ]
}
